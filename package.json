{
   "name": "dsa-js",
   "private": false,
   "version": "0.0.0",
   "type": "module",
   "scripts": {
      "dev": "vite",
      "build": "tsc && vite build",
      "preview": "vite preview",
      "test": "vitest"
   },
   "devDependencies": {
      "@types/node": "^20.12.12",
      "glob": "^10.4.1",
      "type-fest": "^4.18.2",
      "vite-plugin-dts": "^3.9.1",
      "vitest": "^1.6.0",
      "typescript": "^5.5.3",
      "vite": "^5.4.0"
   },
   "main": "./dist/cjs/index.js",
   "module": "./dist/esm/index.js",
   "types": "./dist/types/index.d.ts",
   "exports": {
      ".": {
         "import": "./dist/esm/index.js",
         "require": "./dist/cjs/index.js",
         "types": "./dist/types/index.d.ts"
      },
      "./data-structures/*": {
         "import": "./dist/esm/data-structures/*.js",
         "require": "./dist/cjs/data-structures/*.js",
         "types": "./dist/types/data-structures/*.d.ts"
      },
      "./data-structures": {
         "import": "./dist/esm/data-structures/index.js",
         "require": "./dist/cjs/data-structures/index.js",
         "types": "./dist/types/data-structures/index.d.ts"
      },
      "./algorithms/*": {
         "import": "./dist/esm/algorithms/*.js",
         "require": "./dist/cjs/algorithms/*.js",
         "types": "./dist/types/algorithms/*.d.ts"
      },
      "./algorithms": {
         "import": "./dist/esm/algorithms/index.js",
         "require": "./dist/cjs/algorithms/index.js",
         "types": "./dist/types/algorithms/index.d.ts"
      }
   },
   "files": [
      "dist"
   ]
}
